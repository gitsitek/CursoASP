<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <connectionStrings>
        <add name="CursoASPConnectionString" connectionString="Data Source=localhost;Initial Catalog=CursoASP;Integrated Security=True"
            providerName="System.Data.SqlClient" />
    </connectionStrings>
    <system.webServer>
        <defaultDocument>
            <files>
                <add value="index.asp" />
            </files>
        </defaultDocument>
    </system.webServer>
</configuration>
