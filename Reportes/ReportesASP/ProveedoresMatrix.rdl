  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>CursoASP</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@moral">
            <Value>=Parameters!moral.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@localidad_id">
            <Value>=Parameters!localidad_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        Proveedores.Id, Proveedores.Nombre, Proveedores.RFC, Proveedores.Moral, ciudades.nombre AS ciudad, estados.nombre AS estado, ISNULL(Adeudos.monto, CAST(0 AS numeric(10, 2))) AS monto
FROM            Proveedores INNER JOIN
                         Localidades AS ciudades ON Proveedores.localidad_id = ciudades.Id INNER JOIN
                         Localidades AS estados ON ciudades.padre_id = estados.Id LEFT OUTER JOIN
                         Adeudos ON Proveedores.Id = Adeudos.proveedor_id
WHERE        (Proveedores.Moral = COALESCE (@moral, Proveedores.Moral)) AND (Proveedores.localidad_id IN (@localidad_id))</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
